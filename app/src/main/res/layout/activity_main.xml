<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical">

    <TextView
        android:id="@+id/y"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Accelerometer"
        android:textSize="20dp"
        tools:ignore="UnknownId" />

    <TextView
        android:id="@+id/yValue"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/xValue"
        android:layout_alignEnd="@+id/xValue"
        android:layout_alignRight="@+id/xValue"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/xValue"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/y"
        android:layout_marginLeft="30dp"
        android:layout_marginTop="4dp"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/y"
        android:layout_marginTop="7dp"
        android:text="X" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView"
        android:layout_marginTop="13dp"
        android:text="Y" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="11dp"
        android:text="Z" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mh2"
        android:layout_marginTop="10dp"
        android:text="X" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView15"
        android:layout_marginTop="10dp"
        android:text="Z" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="24dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView14"
        android:layout_marginTop="10dp"
        android:text="Y" />

    <TextView
        android:id="@+id/zValue"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/yValue"
        android:layout_alignEnd="@+id/xValue"
        android:layout_alignRight="@+id/xValue"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="0dp"
        android:layout_marginRight="0dp"
        android:textSize="20dp" />
    <TextView
        android:id="@+id/mh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView3"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="-10dp"
        android:text="Filtrasi Accelerometer"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/mh2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView13"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="-10dp"
        android:text="Gyro"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/xfilter"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/mh"
        android:layout_marginStart="17dp"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="16dp"
        android:layout_toEndOf="@+id/textView7"
        android:layout_toRightOf="@+id/textView7"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/yfilter"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/xfilter"
        android:layout_marginStart="18dp"
        android:layout_marginLeft="18dp"
        android:layout_marginTop="1dp"
        android:layout_toEndOf="@+id/textView8"
        android:layout_toRightOf="@+id/textView8"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/zfilter"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/yfilter"
        android:layout_marginStart="19dp"
        android:layout_marginLeft="19dp"
        android:layout_marginTop="5dp"
        android:layout_toEndOf="@+id/textView9"
        android:layout_toRightOf="@+id/textView9"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/xxfilter"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/zfilter"
        android:layout_marginStart="21dp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="0dp"
        android:layout_toEndOf="@+id/textView10"
        android:layout_toRightOf="@+id/textView10"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/yyfilter"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/xxfilter"
        android:layout_marginStart="21dp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="-1dp"
        android:layout_toEndOf="@+id/textView11"
        android:layout_toRightOf="@+id/textView11"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/zzfilter"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_below="@id/yyfilter"
        android:layout_marginStart="21dp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="-1dp"
        android:layout_toEndOf="@+id/textView12"
        android:layout_toRightOf="@+id/textView12"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/acctot"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/zzfilter"
        android:layout_alignBottom="@+id/zzfilter"
        android:layout_marginStart="29dp"
        android:layout_marginLeft="29dp"
        android:layout_marginTop="13dp"
        android:layout_marginBottom="-39dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mh"
        android:layout_marginTop="17dp"
        android:text="High-pass X" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mh"
        android:layout_marginTop="43dp"
        android:text="High-pass Y" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mh"
        android:layout_marginTop="69dp"
        android:text="High-pass Z" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mh"
        android:layout_marginTop="129dp"
        android:text="Low-pass Y" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mh"
        android:layout_marginTop="106dp"
        android:text="Low-pass X" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/textView11"
        android:layout_marginTop="6dp"
        android:text="Low-pass Z" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView12"
        android:layout_marginBottom="-42dp"
        android:text="Magnitude" />

    <TextView
        android:id="@+id/gyrox"
        android:layout_width="188dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/mh2"
        android:layout_marginStart="9dp"
        android:layout_marginLeft="9dp"
        android:layout_marginTop="17dp"
        android:layout_toEndOf="@+id/textView14"
        android:layout_toRightOf="@+id/textView14"
        android:text="TextView" />

    <TextView
        android:id="@+id/gyroy"
        android:layout_width="188dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/gyrox"
        android:layout_marginStart="9dp"
        android:layout_marginLeft="9dp"
        android:layout_marginTop="9dp"
        android:layout_toEndOf="@+id/textView14"
        android:layout_toRightOf="@+id/textView14"
        android:text="TextView" />

    <TextView
        android:id="@+id/gyroz"
        android:layout_width="188dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/gyroy"
        android:layout_marginStart="7dp"
        android:layout_marginLeft="7dp"
        android:layout_marginTop="11dp"
        android:layout_toEndOf="@+id/textView14"
        android:layout_toRightOf="@+id/textView14"
        android:text="TextView" />

    <Button
        android:id="@+id/button"
        android:layout_width="105dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/gyroz"
        android:layout_marginTop="21dp"
        android:text="Buka File" />

    <Button
        android:id="@+id/buttonstop"
        android:layout_width="119dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/gyroz"
        android:layout_marginStart="21dp"
        android:layout_marginLeft="21dp"
        android:layout_marginTop="21dp"
        android:layout_toEndOf="@+id/button"
        android:layout_toRightOf="@+id/button"
        android:text="Berhenti" />

</RelativeLayout>